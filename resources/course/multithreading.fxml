<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="course.controllers.MultithreadingController">
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                <Button fx:id="start" layoutX="155.0" layoutY="64.0" mnemonicParsing="false" onAction="#start" prefHeight="25.0" prefWidth="79.0" text="Start" />
                <TextArea fx:id="logging" editable="false" layoutX="294.0" layoutY="14.0" prefHeight="373.0" prefWidth="323.0" />
                <Button fx:id="menu" layoutX="130.0" layoutY="133.0" mnemonicParsing="false" text="Return to main menu" />
            <Button fx:id="reset" layoutX="155.0" layoutY="101.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="79.0" text="Reset" />
            </children>
        </AnchorPane>
    </children>
</VBox>
