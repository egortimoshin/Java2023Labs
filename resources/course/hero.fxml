            <TextArea id="text" fx:id="text" editable="false" layoutX="176.0" layoutY="67.0" prefHeight="275.0" prefWidth="450.0" promptText="Hero is..." />
            <ChoiceBox fx:id="strategy" layoutX="14.0" layoutY="67.0" prefWidth="150.0" />
            <Button fx:id="menu" layoutX="457.0" layoutY="29.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="135.0" text="Return to main menu" />
      </children>
    </AnchorPane>
  </children>
</VBox>
